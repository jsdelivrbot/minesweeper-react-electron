{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "program": "${workspaceFolder}/src/electron.js",
            "protocol": "inspector",
            "env": {
                "ELECTRON_START_URL": "http://localhost:1234/"
            }
        }
    ]
}
